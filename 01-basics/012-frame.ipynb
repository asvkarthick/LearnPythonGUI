   "source": [
    "from tkinter import *\n",
    "\n",
    "root = Tk()\n",
    "root.title(\"Learn Frame\")\n",
    "\n",
    "frame = LabelFrame(root, text=\"This is frame\", padx = 5, pady = 5)\n",
    "frame.pack(padx = 10, pady = 10)\n",
    "\n",
    "b = Button(frame, text = \"My Button\")\n",
    "b.pack()\n",
    "\n",
    "root.mainloop()"
   ]
  }
