    "from tkinter import *\n",
    "\n",
    "root = Tk()\n",
    "root.title(\"Slider demo\")\n",
    "\n",
    "vertical = Scale(root, from_=0, to=200)\n",
    "vertical.pack()\n",
    "\n",
    "horizontal = Scale(root, from_=0, to=200, orient=HORIZONTAL)\n",
    "horizontal.pack()\n",
    "\n",
    "root.mainloop()"
   ]
  }
