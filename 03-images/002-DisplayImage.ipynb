   "source": [
    "from tkinter import *\n",
    "from PIL import ImageTk, Image\n",
    "\n",
    "root = Tk()\n",
    "root.title(\"Display Image\")\n",
    "root.iconbitmap(\"data/icon.png\")\n",
    "\n",
    "myImage = ImageTk.PhotoImage(Image.open(\"data/Lenna.png\"))\n",
    "myLabel = Label(image=myImage)\n",
    "myLabel.pack()\n",
    "\n",
    "root.mainloop()"
   ]
  }
